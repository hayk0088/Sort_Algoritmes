#Solution N1
def buble_sort(list):
    for x in range(len(list)-1):
        for i in range(len(list)-1-x):
            if list[i] > list[i+1]:
                list[i],list[i+1] = list[i+1],list[i]
    return list
    
#Solution N2
def buble_sort(arr):
    for k in range(1,len(arr)):
        for i in range(len(arr)-k):
            if arr[i] > arr[i+1]:
                arr[i],arr[i+1] = arr[i+1],arr[i]
    return arr
